class GoTile {
    field int oiiai;

    constructor GoTile new() {
        return this;
    }

    method void print(String msg, int row) {
        do Output.moveCursor(7+row, 7);
        do Output.printString(msg);
        do msg.dispose();
        return;
    }

    method void activate(Player p) {
        do print("GO tile!!", 0);
        do print("earned $200!", 1);
        do p.receive(200);
        do Sys.wait(300);
        return;
    }
}
